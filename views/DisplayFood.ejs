<style>
    .div{
    
        background-image: url('/images/black2.png');
        background-repeat: no-repeat;
            background-size: cover;
            color: white;
            height: 100vh;
           
    }
    .container{
        width: 300vh; 
        display: flex;
        justify-content: center;
        align-items: center;
        color: white;
        font-family: kalnia;
        font-weight: 200;
        font-size: 120px;
        margin-top: 3%;
       
    }
    img{
        border-radius: 50px;
        cursor: pointer;
    }
    .heading{
        color: white;
        margin-top: 2%;
    }
    .box{
        display: flex;
       justify-content: space-between;
       color: black;
    }
</style>

<head>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
 <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
 <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.min.js" integrity="sha384-BBtl+eGJRgqQAUMxJ7pMwbEyER4l1g+O15P+16Ep7Q9Q+zqX6gSbd85u4mG4QzX+" crossorigin="anonymous"></script>
 <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
 <script src="/javascripts/projects.js"></script>
 <link rel="preconnect" href="https://fonts.googleapis.com">
 <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
 <link href="https://fonts.googleapis.com/css2?family=Kalnia:wght@100;200;300;400;500&family=Poppins:wght@300&family=Roboto:wght@500&display=swap" rel="stylesheet">
 <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
 
 </head>
<html>
    <div class="div">
    <div class="box">

        <img src="/images/icon7.png" alt="f" width="100px" height="100px">
        <h2 class="heading">ORDER LIST</h2>
        <a href="/interface/foodinterface">
        <img src="/images/icon5.png" alt="" width="100px" height="100px">
            </a>
     </div>
    <div class="container">
       
    <table class="table table-dark table-striped">
        <thead>
          <tr>
            <th scope="col">Food id</th>
            <th scope="col">Food Type</th>
            <th scope="col">Hotel</th>
            <th scope="col">Dishes</th>
            <th scope="col">Quantity</th>
            <th scope="col">Price/Offer</th>
            <th scope="col">Date/Time</th>
            <th scope="col">Status</th>
            <th scope="col">Poster</th>
            <th scope="col">Edit/Delete</th>
          </tr>
        </thead>
        <tbody>
            <% data.map((item)=>{ %>
          <tr>
            <th scope="row"><%=item.foodid %></th>
            <td><%=item.foodtype %></td>
            <td><%=item.hotels %></td>
            <td><%=item.dishes %></td>
            <td><%=item.qty %></td>
            <td><%=item.prize %><br><%=item.offer %></td>
            <td><%=item.date %><br><%=item.time %></td>
            <td><%=item.status %></td>
            <td> <a href="/interface/displayposter?foodid=<%=item.foodid %>&foodtypes=<%=item.foodtype%>&poster=<%=item.poster %>"><img src="/images/<%=item.poster%>" style="width: 80px; height: 70px;"/></a></td>
            <td><a href="/interface/edit_food_data?foodid=<%=item.foodid %>" style="text-decoration: none; color: white;"> Edit/Delete </a></td>
            
          </tr>
          <% }) %>
       </tbody>
      </table>
    </div>
</div>
</html>